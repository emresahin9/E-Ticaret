@model IEnumerable<E_Ticaret.Models.Admin>
@{
    Layout = "~/Views/Shared/_Admin_Layout.cshtml";
}

<!DOCTYPE html>

<html lang="tr">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Yöneticiler</title>
</head>
<body>
    <h2>Mevcut Yönetici Hesapları</h2>
    <hr />
    <table class="table table-striped table-bordered table-hover table-active table-responsive-md">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model=>model.adminFullname)</th>
                <th>@Html.DisplayNameFor(model => model.adminEmail)</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.adminFullname</td>
                <td>@item.adminEmail</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>
